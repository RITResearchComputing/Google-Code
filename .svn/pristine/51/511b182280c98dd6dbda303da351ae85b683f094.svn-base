body {
background-color: #000066;
margin-top: 4%;
margin-bottom: auto;
margin-left: auto;
margin-right: auto;
vertical-align: middle;
font-family: "Lucida Grande", "Bitstream Vera Sans", "Verdana";
font-size: 10.5pt;
}

.leftColumn {
float:left;
width:49%;
margin-top: 1%;
text-align: center;
}
.rightColumn {
float:left;
width:49%;
margin-top: 1%;
padding-left: 0px;
text-align:center;
}
.columnLayout {
padding-bottom:10%;
margin: 0 auto;
}
.navigation {
   font-size: .8em !important;
}

 
.space {
padding: 0px;
margin: 0px;
float:left;
width:1%;
}
#accordion {
font-size:10.5pt;
}
#main {
padding: 0;
background-color: #ffffff;
background-repeat: no-repeat;
font-size: 13px;
color: #333;
}

h1 {
font-size: 28px;
color: #000;
}

a  {
color: #03c
}

#page{
background-color: #ffffff;
width: 900px;
margin: auto;
margin-left: auto;
padding-left: 0px;
margin-right: auto;
-moz-border-radius: 15px;
border-radius: 15px;
border:5px solid;
}
#admin a {
text-align:center;
}
#submission {
padding: 30px;
}
#content {
background-color: white;
margin-right: 1%;
margin-left: 1%;
}

#sidebar {
float: right;
width: 175px;
}

#header, #footer {
text-align: center;
display: block;
margin-left: auto;
margin-right: auto;
clear:both;
}
#header h1, #header h2 {margin: 0}
#header h2 {
color: #000;
font-weight: normal;
font-size: 16px;
}


h1, h2, h3, h4, h5 {
font-family: sans-serif;
margin: 0 0 0 0;
}

p {
line-height: 1.5em;
margin: 0 0;
}

code, tt {
    font-family: 'Andale Mono', Monaco, 'Liberation Mono', 'Bitstream Vera Sans Mono', 'DejaVu Sans Mono', monospace;
}

#footer {
text-align: center;
font-family: sans-serif;
font-size: 10px;
}

 .ui-button-text{ 
    text-align: center; 
    float: none;
}

