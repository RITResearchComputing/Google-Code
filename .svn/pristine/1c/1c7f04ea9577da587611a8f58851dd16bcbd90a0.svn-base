#!/bin/bash

can_see=$(df -h | grep photoshare | wc -l)

if [ "${can_see}" -eq "1" ] ; then
        echo "CHECK_SMBPHOTOS OK"
        exit 0
fi
if [ "${can_see}" -eq "0" ] ; then
        echo "CHECK_SMBPHOTOS CRIT - no photoshare entry in df output"
        exit 2
fi

echo "CHECK_SMBPHOTOS WARN - not sure whats going on"
exit 1
